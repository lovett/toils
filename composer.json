{
    "name": "lovett/toils",
    "description": "Time tracking and invoice management for consultants",
    "keywords": [
        "admin",
        "hours",
        "invoicing"
    ],
    "homepage": "https://github.com/lovett/toils/",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Bill Lovett",
            "email": "bill@ilovett.com",
            "homepage": "https://ilovett.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "v1.0.0",
        "erusev/parsedown": "1.7.4",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "v3.0.0",
        "guzzlehttp/guzzle": "7.4.2",
        "laravel/framework": "~8.82.0",
        "laravel/helpers": "v1.5.0",
        "laravel/tinker": "v2.7.2",
        "laravel/ui": "v3.4.5",
        "laravelcollective/html": "v6.3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "v3.6.7",
        "beyondcode/laravel-dump-server": "1.8.0",
        "filp/whoops": "2.14.5",
        "fakerphp/faker": "v1.19.0",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "~5.10.0",
        "nunomaduro/larastan": "~1.0.3",
        "phpmd/phpmd": "2.12.0",
        "phpunit/phpunit": "9.5.20",
        "squizlabs/php_codesniffer": "3.6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
