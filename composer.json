{
    "name": "lovett/toils",
    "description": "Time tracking and invoice management for consultants",
    "keywords": ["admin", "hours", "invoicing"],
    "homepage": "https://github.com/lovett/toils/",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Bill Lovett",
            "email": "bill@ilovett.com",
            "homepage": "https://ilovett.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-dompdf": "0.8.3",
        "fideloper/proxy": "4.1.0",
        "laravel/framework": "5.7.22",
        "laravel/tinker": "1.0.8",
        "laravelcollective/html": "5.7.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.1",
        "beyondcode/laravel-dump-server": "1.2.2",
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.0",
        "nunomaduro/collision": "2.1.1",
        "nunomaduro/larastan": "0.3.15",
        "phpmd/phpmd": "2.6.0",
        "phpunit/phpunit": "7.5.2",
        "squizlabs/php_codesniffer": "3.4.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
