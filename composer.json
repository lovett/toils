{
    "name": "lovett/toils",
    "description": "Time tracking and invoice management for consultants",
    "keywords": [
        "admin",
        "hours",
        "invoicing"
    ],
    "homepage": "https://github.com/lovett/toils/",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Bill Lovett",
            "email": "bill@ilovett.com",
            "homepage": "https://ilovett.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "barryvdh/laravel-dompdf": "v0.8.7",
        "erusev/parsedown": "1.7.4",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "v2.0.2",
        "guzzlehttp/guzzle": "7.0.1",
        "laravel/framework": "v8.4.0",
        "laravel/helpers": "v1.3.0",
        "laravel/tinker": "v2.4.2",
        "laravel/ui": "v3.0.0",
        "laravelcollective/html": "v6.2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "v3.5.1",
        "beyondcode/laravel-dump-server": "1.5.0",
        "filp/whoops": "2.7.3",
        "fzaninotto/faker": "v1.9.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "v5.0.2",
        "nunomaduro/larastan": "v0.6.4",
        "phpmd/phpmd": "2.9.0",
        "phpunit/phpunit": "9.3.10",
        "squizlabs/php_codesniffer": "3.5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
