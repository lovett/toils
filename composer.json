{
    "name": "lovett/toils",
    "description": "Time tracking and invoice management for consultants",
    "keywords": ["admin", "hours", "invoicing"],
    "homepage": "https://github.com/lovett/toils/",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Bill Lovett",
            "email": "bill@ilovett.com",
            "homepage": "https://ilovett.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "barryvdh/laravel-dompdf": "0.8.6",
        "erusev/parsedown": "1.7.4",
        "fideloper/proxy": "4.3.0",
        "fruitcake/laravel-cors": "1.0.5",
        "guzzlehttp/guzzle": "6.5.2",
        "laravel/framework": "7.1.2",
        "laravel/helpers": "1.2.0",
        "laravel/tinker": "2.2.0",
        "laravel/ui": "2.0.1",
        "laravelcollective/html": "6.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.9",
        "beyondcode/laravel-dump-server": "1.4.0",
        "filp/whoops": "2.7.1",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "4.1.3",
        "nunomaduro/larastan": "0.5.2",
        "phpmd/phpmd": "2.8.2",
        "phpunit/phpunit": "9.0.1",
        "squizlabs/php_codesniffer": "3.5.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
