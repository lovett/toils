{
    "name": "lovett/toils",
    "description": "Time tracking and invoice management for consultants",
    "keywords": [
        "admin",
        "hours",
        "invoicing"
    ],
    "homepage": "https://github.com/lovett/toils/",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Bill Lovett",
            "email": "bill@ilovett.com",
            "homepage": "https://ilovett.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "barryvdh/laravel-dompdf": "v0.8.6",
        "erusev/parsedown": "1.7.4",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "v2.0.1",
        "guzzlehttp/guzzle": "7.0.1",
        "laravel/framework": "v7.21.0",
        "laravel/helpers": "v1.2.0",
        "laravel/tinker": "v2.4.1",
        "laravel/ui": "v2.1.0",
        "laravelcollective/html": "v6.1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "v3.3.3",
        "beyondcode/laravel-dump-server": "1.4.0",
        "filp/whoops": "2.7.3",
        "fzaninotto/faker": "v1.9.1",
        "mockery/mockery": "1.4.1",
        "nunomaduro/collision": "v4.2.0",
        "nunomaduro/larastan": "v0.6.1",
        "phpmd/phpmd": "2.8.2",
        "phpunit/phpunit": "9.2.6",
        "squizlabs/php_codesniffer": "3.5.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
